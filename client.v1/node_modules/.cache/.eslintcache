ableWarningCount":0,"source":null},{"filePath":"224","messages":"225","errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"226","messages":"227","errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/index.js",[],"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/App.js",["228"],"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/components/AppContext.js",[],"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/components/AppRouter.js",[],"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/components/NavBar.js",[],"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/store/BasketStore.js",[],"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/store/CatalogStore.js",[],"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/store/UserStore.js",[],"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/components/CheckAuth.js",["229"],"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/components/FetchBasket.js",["230"],"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/pages/Basket.js",[],"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/pages/Login.js",["231"],"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/pages/Shop.js",["232","233","234"],"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/pages/Signup.js",["235","236"],"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/pages/Checkout.js",["237","238"],"import { Container, Form, Button, Spinner } from 'react-bootstrap'\nimport { useState, useContext, useEffect } from 'react'\nimport { AppContext } from '../components/AppContext.js'\nimport { userCreate, guestCreate } from '../http/orderAPI.js'\nimport { fetchBasket } from '../http/basketAPI.js'\nimport { check as checkAuth } from '../http/userAPI.js'\nimport { Navigate } from 'react-router-dom'\n\nconst isValid = (input) => {\n    let pattern\n    switch (input.name) {\n        case 'name':\n            pattern = /^[-a-z]{2,}( [-a-z]{2,}){1,2}$/i\n            return pattern.test(input.value.trim())\n        case 'email':\n            pattern = /^[-_.a-z]+@([-a-z]+\\.){1,2}[a-z]+$/i\n            return pattern.test(input.value.trim())\n        case 'phone':\n            pattern = /^(\\s*)?(\\+)?([- _():=+]?\\d[- _():=+]?){10,14}(\\s*)?$/\n            return pattern.test(input.value.trim())\n        case 'address':\n            return input.value.trim() !== ''\n    }\n}\n\nconst Checkout = () => {\n    const { user, basket } = useContext(AppContext)\n    const [fetching, setFetching] = useState(true) // loader, пока получаем корзину\n\n    const [order, setOrder] = useState(null)\n\n    const [value, setValue] = useState({name: '', email: '', phone: '', address: ''})\n    const [valid, setValid] = useState({name: null, email: null, phone: null, address: null})\n\n    useEffect(() => {\n        // если корзина пуста, здесь делать нечего\n        fetchBasket()\n            .then(\n                data => basket.products = data.products\n            )\n            .finally(\n                () => setFetching(false)\n            )\n        // нужно знать, авторизован ли пользователь\n        checkAuth()\n            .then(data => {\n                if (data) {\n                    user.login(data)\n                }\n            })\n            .catch(\n                error => user.logout()\n            )\n    }, [])\n\n    if (fetching) { // loader, пока получаем корзину\n        return <Spinner animation=\"border\" />\n    }\n\n    if (order) { // заказ был успешно оформлен\n        return (\n            <Container>\n                <div className=\"mb-3\">\n                    <div className=\"page-header d-flex align-items-center\">\n                        <div className=\"container position-relative\">\n                            <div className=\"row d-flex justify-content-center\">\n                                <div className=\"col-lg-10 text-center\">\n                <h1 className=\"mb-4 mt-4\">Order is processed</h1>\n                <p>We will email you soon for details.</p>\n                                </div>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n            </Container>\n        )\n    }\n\n    const handleChange = (event) => {\n        setValue({...value, [event.target.name]: event.target.value})\n        /*\n         * Вообще говоря, проверять данные поля, пока пользователь не закончил ввод — неправильно,\n         * проверять надо в момент потери фокуса. Но приходится проверять здесь, поскольку браузеры\n         * автоматически заполняют поля. И отловить это событие — весьма проблематичная задача.\n         */\n        setValid({...valid, [event.target.name]: isValid(event.target)})\n    }\n\n    const handleSubmit = (event) => {\n        event.preventDefault()\n\n        setValue({\n            name: event.target.name.value.trim(),\n            email: event.target.email.value.trim(),\n            phone: event.target.phone.value.trim(),\n            address: event.target.address.value.trim(),\n        })\n\n        setValid({\n            name: isValid(event.target.name),\n            email: isValid(event.target.email),\n            phone: isValid(event.target.phone),\n            address: isValid(event.target.address),\n        })\n\n        if (valid.name && valid.email && valid.phone && valid.address) {\n            let comment = event.target.comment.value.trim()\n            comment = comment ? comment : null\n            // форма заполнена правильно, можно отправлять данные\n            const body = {...value, comment}\n            const create = user.isAuth ? userCreate : guestCreate\n            create(body)\n                .then(\n                    data => {\n                        setOrder(data)\n                        basket.products = []\n                    }\n                )\n        }\n    }\n\n    return (\n        <Container className=\"page-header\">\n            {basket.count === 0 && <Navigate to=\"/basket\" replace={true} />}\n            <h1 className=\"mb-4 mt-4\">Order request</h1>\n            <Form noValidate onSubmit={handleSubmit}>\n                <Form.Control\n                    name=\"name\"\n                    value={value.name}\n                    onChange={e => handleChange(e)}\n                    isValid={valid.name === true}\n                    isInvalid={valid.name === false}\n                    placeholder=\"First name & Second name\"\n                    className=\"mb-3\"\n                />\n                <Form.Control\n                    name=\"email\"\n                    value={value.email}\n                    onChange={e => handleChange(e)}\n                    isValid={valid.email === true}\n                    isInvalid={valid.email === false}\n                    placeholder=\"Email\"\n                    className=\"mb-3\"\n                />\n                <Form.Control\n                    name=\"phone\"\n                    value={value.phone}\n                    onChange={e => handleChange(e)}\n                    isValid={valid.phone === true}\n                    isInvalid={valid.phone === false}\n                    placeholder=\"Phone number\"\n                    className=\"mb-3\"\n                />\n                <Form.Control\n                    name=\"address\"\n                    value={value.address}\n                    onChange={e => handleChange(e)}\n                    isValid={valid.address === true}\n                    isInvalid={valid.address === false}\n                    placeholder=\"Address\"\n                    className=\"mb-3\"\n                />\n                <Form.Control\n                    name=\"comment\"\n                    className=\"mb-3\"\n                    placeholder=\"Comment\"\n                />\n                <Button type=\"submit\">Send</Button>\n            </Form>\n        </Container>\n    )\n}\n\nexport default Checkout",["239","240"],"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/pages/Product.js",["241","242"],"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/pages/Delivery.js",[],"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/pages/Contacts.js",["243"],"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/pages/NotFound.js",[],"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/pages/User.js",[],"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/pages/UserOrders.js",[],"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/pages/UserOrder.js",[],"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/pages/Admin.js",[],"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/pages/AdminOrder.js",[],"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/pages/AdminCategories.js",[],"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/pages/AdminOrders.js",[],"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/pages/AdminBrands.js",[],"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/pages/AdminProducts.js",["244","245"],"import { useState, useEffect } from 'react'\nimport { fetchAllProducts, deleteProduct } from '../http/catalogAPI.js'\nimport {Button, Container, Spinner, Table, Pagination, Image} from 'react-bootstrap'\nimport CreateProduct from '../components/CreateProduct.js'\nimport UpdateProduct from '../components/UpdateProduct.js'\n\n// количество товаров на страницу\nconst ADMIN_PER_PAGE = 6\n\nconst AdminProducts = () => {\n    const [products, setProducts] = useState([]) // список загруженных товаров\n    const [fetching, setFetching] = useState(true) // загрузка списка товаров с сервера\n    const [createShow, setCreateShow] = useState(false) // модальное окно создания товара\n    const [updateShow, setUpdateShow] = useState(false) // модальное окно редактирования\n    // для обновления списка после добавления, редактирования, удаления — изменяем состояние\n    const [change, setChange] = useState(false)\n    // id товара, который будем редактировать — для передачи в <UpdateProduct id={…} />\n    const [product, setProduct] = useState(null)\n\n    // текущая страница списка товаров\n    const [currentPage, setCurrentPage] = useState(1)\n    // сколько всего страниц списка товаров\n    const [totalPages, setTotalPages] = useState(1)\n\n    // обработчик клика по номеру страницы\n    const handlePageClick = (page) => {\n        setCurrentPage(page)\n        setFetching(true)\n    }\n\n    // содержимое компонента <Pagination>\n    const pages = []\n    for (let page = 1; page <= totalPages; page++) {\n        pages.push(\n            <Pagination.Item\n                key={page}\n                active={page === currentPage}\n                activeLabel=\"\"\n                onClick={() => handlePageClick(page)}\n            >\n                {page}\n            </Pagination.Item>\n        )\n    }\n\n    const handleUpdateClick = (id) => {\n        setProduct(id)\n        setUpdateShow(true)\n    }\n\n    const handleDeleteClick = (id) => {\n        deleteProduct(id)\n            .then(\n                data => {\n                    // если это последняя страница и мы удаляем на ней единственный\n                    // оставшийся товар — то надо перейти к предыдущей странице\n                    if (totalPages > 1 && products.length === 1 && currentPage === totalPages) {\n                        setCurrentPage(currentPage - 1)\n                    } else {\n                        setChange(!change)\n                    }\n                    alert(`Item «${data.name}» was deleted`)\n                }\n            )\n            .catch(\n                error => alert(error.response.data.message)\n            )\n    }\n\n    useEffect(() => {\n        fetchAllProducts(null, null, currentPage, ADMIN_PER_PAGE)\n            .then(\n                data => {\n                    setProducts(data.rows)\n                    setTotalPages(Math.ceil(data.count / ADMIN_PER_PAGE))\n                }\n            )\n            .finally(\n                () => setFetching(false)\n            )\n    }, [change, currentPage])\n\n    if (fetching) {\n        return <Spinner animation=\"border\" />\n    }\n\n    return (\n        <Container className=\"page-header\">\n            <h1>Items</h1>\n            <Button onClick={() => setCreateShow(true)}>Create item</Button>\n            <CreateProduct show={createShow} setShow={setCreateShow} setChange={setChange} />\n            <UpdateProduct id={product} show={updateShow} setShow={setUpdateShow} setChange={setChange} />\n            {products.length > 0 ? (\n                <>\n                    <Table bordered hover size=\"sm\" className=\"mt-3\">\n                    <thead>\n                        <tr>\n                            <th>Name</th>\n                            <th>Photo</th>\n                            <th>Category</th>\n                            <th>Type</th>\n                            <th>Price</th>\n                            <th>Manage</th>\n                            <th>Delete</th>\n                        </tr>\n                    </thead>\n                    <tbody>\n                        {products.map(item => \n                            <tr key={item.id}>\n                                <td>{item.name}</td>\n                                <td>\n                                    {item.image &&\n                                        <a href={process.env.REACT_APP_IMG_URL + item.image} target=\"_blank\">Photo</a>\n                                        }\n                                </td>\n                                <td>{item.category?.name || 'NULL'}</td>\n                                <td>{item.brand?.name || 'NULL'}</td>\n                                <td>{item.price}</td>\n                                <td>\n                                    <Button variant=\"success\" size=\"sm\" onClick={() => handleUpdateClick(item.id)}>\n                                        Manage\n                                    </Button>\n                                </td>\n                                <td>\n                                    <Button variant=\"danger\" size=\"sm\" onClick={() => handleDeleteClick(item.id)}>\n                                        Delete\n                                    </Button>\n                                </td>\n                            </tr>\n                        )}\n                    </tbody>\n                    </Table>\n                    {totalPages > 1 && <Pagination>{pages}</Pagination>}\n                </>\n            ) : (\n                <p>Items list is empty</p>\n            )}\n        </Container>\n    )\n}\n\nexport default AdminProducts","/Users/das/Documents/FrontEnd/JsShop/client.v1/src/components/BrandBar.js",["246"],"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/components/BasketList.js",["247"],"import { useContext, useState, useEffect } from 'react'\nimport { AppContext } from './AppContext.js'\nimport { fetchBasket, increment, decrement, remove } from '../http/basketAPI.js'\nimport { Table, Spinner, Button } from 'react-bootstrap'\nimport { useNavigate } from 'react-router-dom'\nimport BasketItem from './BasketItem.js'\nimport { observer } from 'mobx-react-lite'\n\nconst BasketList = observer(() => {\n    const { basket } = useContext(AppContext)\n    const [fetching, setFetching] = useState(true)\n\n    const navigate = useNavigate()\n\n    const handleIncrement = (id) => {\n        setFetching(true)\n        increment(id)\n            .then(\n                data => basket.products = data.products\n            )\n            .finally(\n                () => setFetching(false)\n            )\n    }\n\n    const handleDecrement = (id) => {\n        setFetching(true)\n        decrement(id)\n            .then(\n                data => basket.products = data.products\n            )\n            .finally(\n                () => setFetching(false)\n            )\n    }\n\n    const handleRemove = (id) => {\n        setFetching(true)\n        remove(id)\n            .then(\n                data => basket.products = data.products\n            )\n            .finally(\n                () => setFetching(false)\n            )\n    }\n\n    useEffect(() => {\n        fetchBasket()\n            .then(\n                data => basket.products = data.products\n            )\n            .finally(\n                () => setFetching(false)\n            )\n    }, [])\n\n    if (fetching) {\n        return <Spinner animation=\"border\" />\n    }\n\n    return (\n        <>\n            {basket.count ? (\n                <>\n                    <Table bordered hover size=\"sm\" className=\"mt-3\">\n                        <thead>\n                            <tr>\n                                <th>Title</th>\n                                <th>Quantity</th>\n                                <th>Price</th>\n                                <th>Total</th>\n                                <th>Delete</th>\n                            </tr>\n                        </thead>\n                        <tbody>\n                            {basket.products.map(item => \n                                <BasketItem\n                                    key={item.id}\n                                    increment={handleIncrement}\n                                    decrement={handleDecrement}\n                                    remove={handleRemove}\n                                    {...item}\n                                />\n                            )}\n                            <tr>\n                                <th colSpan=\"3\">Total</th>\n                                <th>{basket.sum}</th>\n                                <th>USD</th>\n                            </tr>\n                        </tbody>\n                    </Table>\n                    <Button onClick={() => navigate('/checkout')}>Checkout</Button>\n                </>\n            ) : (\n                <p>Your basket is empty</p>\n            )}\n        </>\n    )\n})\n\nexport default BasketList","/Users/das/Documents/FrontEnd/JsShop/client.v1/src/components/CategoryBar.js",["248"],"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/components/Orders.js",[],"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/components/ProductList.js",[],"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/components/Order.js",[],"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/components/CreateCategory.js",[],"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/components/CreateOrder.js",[],"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/components/UpdateCategory.js",[],"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/http/catalogAPI.js",[],"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/http/basketAPI.js",[],"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/http/orderAPI.js",[],"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/http/userAPI.js",["249"],"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/components/UpdateBrand.js",[],"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/components/CreateBrand.js",[],"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/components/CreateProduct.js",[],"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/components/UpdateProduct.js",[],"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/components/BasketItem.js",[],"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/components/ProductItem.js",[],"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/http/index.js",[],"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/components/CreateProperties.js",["250"],"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/components/UpdateProperties.js",["251","252","253","254","255"],"import { useEffect, useState } from 'react'\nimport { Row, Col, Button, Form } from 'react-bootstrap'\nimport { createProperty, updateProperty, deleteProperty } from '../http/catalogAPI.js'\nimport uuid from 'react-uuid'\n\nconst UpdateProperties = (props) => {\n    const { properties, setProperties } = props\n\n    const append = () => {\n        setProperties([...properties, {id: null, name: '', value: '', unique: uuid(), append: true}])\n    }\n    const remove = (unique) => {\n        // новую хар-ку надо просто удалить из массива properties, а старую — оставить, но\n        // изменить remove на true, чтобы потом выполнить http-запрос на сервер для удаления\n        const item = properties.find(elem => elem.unique === unique)\n        if (item.id) { // старая хар-ка\n            setProperties(properties.map(\n                elem => elem.unique === unique ? {...elem, change: false, remove: true} : elem\n            ))\n        } else { // новая хар-ка\n            setProperties(properties.filter(elem => elem.unique === unique))\n        }\n    }\n    const change = (key, value, unique) => {\n        setProperties(properties.map(\n            item => item.unique === unique ? {...item, [key]: value, change: !item.append} : item\n        ))\n    }\n\n    return (\n        <>\n            <h5>Характеристики</h5>\n            <Button onClick={append} variant=\"outline-primary\" size=\"sm\" className=\"mb-2\">\n                Добавить\n            </Button>\n            {properties.map(item =>\n                <Row key={item.unique} className=\"mb-2\" style={{display: item.remove ? 'none': 'flex'}}>\n                    <Col>\n                        <Form.Control\n                            name={'name_' + item.unique}\n                            value={item.name}\n                            onChange={e => change('name', e.target.value, item.unique)}\n                            placeholder=\"Title...\"\n                            size=\"sm\"\n                        />\n                    </Col>\n                    <Col>\n                        <Form.Control\n                            name={'value_' + item.unique}\n                            value={item.value}\n                            onChange={e => change('value', e.target.value, item.unique)}\n                            placeholder=\"Value...\"\n                            size=\"sm\"\n                        />\n                    </Col>\n                    <Col>\n                        <Button onClick={() => remove(item.unique)} size=\"sm\" variant=\"outline-danger\">\n                            Delete\n                        </Button>\n                        {item.change && ' *'}\n                    </Col>\n                </Row>\n            )}\n        </>\n    )\n}\n\nexport default UpdateProperties","/Users/das/Documents/FrontEnd/JsShop/client.v1/src/components/Header.js",["256","257","258","259"],"import React, {useContext} from 'react';\nimport {observer} from \"mobx-react-lite\";\nimport {AppContext} from \"./AppContext\";\nimport {NavLink} from \"react-router-dom\";\nimport FetchBasket from \"./FetchBasket\";\nimport CheckAuth from \"./CheckAuth\";\nimport logo from \"../assets/logo.png\"\n\nconst Header = observer(() => {\n    const { user, basket } = useContext(AppContext)\n    return (\n        <header id=\"header\" className=\"header d-flex align-items-center fixed-top bg-light\">\n            <div className=\"container-fluid d-flex align-items-center justify-content-between\">\n                <NavLink to=\"/\" className=\"logo d-flex align-items-center  me-auto me-lg-0\">\n                    <img src={logo} alt=\"Arina Yastrebova\"/>\n                </NavLink>\n                <nav id=\"navbar\" className=\"navbar\">\n                    <ul>\n                        <li><NavLink to=\"/delivery\" className=\"nav-link\">Shipping</NavLink></li>\n                        <li><NavLink to=\"/contacts\" className=\"nav-link\">Contact</NavLink></li>\n                        <FetchBasket>\n                            <li><NavLink to=\"/basket\" className=\"nav-link\">\n                                Basket\n                                {!!basket.count && <span>({basket.count})</span>}\n                            </NavLink></li>\n                        </FetchBasket>\n                        <CheckAuth>\n                            {user.isAuth ? (\n                                <NavLink to=\"/user\" className=\"nav-link\">User panel</NavLink>\n                            ) : (\n                                <>\n                                    <li><NavLink to=\"/login\" className=\"nav-link\">Log in</NavLink></li>\n                                    <li><NavLink to=\"/signup\" className=\"nav-link\">Register</NavLink></li>\n                                </>\n                            )}\n                            {user.isAdmin && (\n                                <li><NavLink to=\"/admin\" className=\"nav-link\">Admin</NavLink></li>\n                            )}\n                        </CheckAuth>\n                    </ul>\n\n                </nav>\n\n                <div className=\"header-social-links\">\n                    <a href=\"https://twitter.com/arinayastrebova\" target=\"_blank\" className=\"twitter\"><i className=\"bi bi-twitter\"></i></a>\n                    <a href=\"https://www.facebook.com/ArinaYastrebovaStore/\" target=\"_blank\" className=\"facebook\"><i className=\"bi bi-facebook\"></i></a>\n                    <a href=\"https://www.instagram.com/arinasfinearts/\" target=\"_blank\" className=\"instagram\"><i className=\"bi bi-instagram\"></i></a>\n                    <a href=\"https://ge.linkedin.com/in/arina-iastrebova-39a907253\" target=\"_blank\" className=\"linkedin\"><i className=\"bi bi-linkedin\"></i></a>\n            </div>\n            <i className=\"mobile-nav-toggle mobile-nav-show bi bi-list\"></i>\n            <i className=\"mobile-nav-toggle mobile-nav-hide d-none bi bi-x\"></i>\n\n        </div>\n</header>\n    );\n})\n\nexport default Header;","/Users/das/Documents/FrontEnd/JsShop/client.v1/src/components/Hero.js",[],"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/components/Contact.js",["260"],"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/components/Filters.js",[],"/Users/das/Documents/FrontEnd/JsShop/client.v1/src/components/Slider.js",[],{"ruleId":"261","severity":1,"message":"262","line":3,"column":8,"nodeType":"263","messageId":"264","endLine":3,"endColumn":14},{"ruleId":"265","severity":1,"message":"266","line":20,"column":8,"nodeType":"267","endLine":20,"endColumn":10,"suggestions":"268"},{"ruleId":"265","severity":1,"message":"269","line":18,"column":8,"nodeType":"267","endLine":18,"endColumn":10,"suggestions":"270"},{"ruleId":"265","severity":1,"message":"271","line":16,"column":8,"nodeType":"267","endLine":16,"endColumn":10,"suggestions":"272"},{"ruleId":"261","severity":1,"message":"273","line":2,"column":8,"nodeType":"263","messageId":"264","endLine":2,"endColumn":19},{"ruleId":"261","severity":1,"message":"274","line":4,"column":8,"nodeType":"263","messageId":"264","endLine":4,"endColumn":16},{"ruleId":"261","severity":1,"message":"275","line":12,"column":8,"nodeType":"263","messageId":"264","endLine":12,"endColumn":14},{"ruleId":"261","severity":1,"message":"276","line":4,"column":21,"nodeType":"263","messageId":"264","endLine":4,"endColumn":24},{"ruleId":"265","severity":1,"message":"271","line":16,"column":8,"nodeType":"267","endLine":16,"endColumn":10,"suggestions":"277"},{"ruleId":"278","severity":1,"message":"279","line":11,"column":5,"nodeType":"280","messageId":"281","endLine":23,"endColumn":6},{"ruleId":"265","severity":1,"message":"282","line":54,"column":8,"nodeType":"267","endLine":54,"endColumn":10,"suggestions":"283"},{"ruleId":"284","replacedBy":"285"},{"ruleId":"286","replacedBy":"287"},{"ruleId":"261","severity":1,"message":"288","line":1,"column":55,"nodeType":"263","messageId":"264","endLine":1,"endColumn":60},{"ruleId":"261","severity":1,"message":"289","line":12,"column":12,"nodeType":"263","messageId":"264","endLine":12,"endCol